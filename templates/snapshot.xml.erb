<volume>
    <name><%= ENV['vol_name'] %></name>
    <capacity unit='G'><%= ENV['vol_size'] %></capacity>
    <target>
        <format type='<%= ENV['format'] %>'/>
    </target>
    <backingStore>
        <path><%= ENV['backing_path'] %></path>
        <format type='<%= ENV['backing_format'] %>'/>
    </backingStore>
</volume>
